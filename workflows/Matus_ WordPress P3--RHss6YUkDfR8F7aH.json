Boolean);\n\n// === FUNCIONES AUXILIARES PARA SLUG ===\nconst removeEmojis = (text) => {\n  return text.replace(/([\\u2700-\\u27BF]|[\\uE000-\\uF8FF]|[\\uD83C-\\uDBFF\\uDC00-\\uDFFF])/g, '');\n};\n\nconst slugify = (text) => {\n  return removeEmojis(text)\n    .toLowerCase()\n    .normalize(\"NFD\")\n    .replace(/[\\u0300-\\u036f]/g, \"\")\n    .replace(/[^a-z0-9 ]/g, \"\")\n    .trim()\n    .replace(/\\s+/g, \"-\");\n};\n\nconst slug = slugify($json.Titulo || '');\n\n// === PALABRAS CLAVE POR CATEGOR√çA ===\nconst mapaCategorias = {\n  'Coaching Empresarial': ['coaching', 'mentor', 'acompa√±amiento', 'sesi√≥n', 'ejecutivo'],\n  'Empresa Familiar': ['empresa familiar', 'herencia', 'familiares', 'transici√≥n generacional'],\n  'Liderazgo': ['l√≠der', 'liderazgo', 'inspirar', 'decisi√≥n', 'visi√≥n'],\n  'Plan de Sucesi√≥n': ['sucesi√≥n', 'sucesor', 'heredero', 'transici√≥n', 'plan sucesorio'],\n  'Profesionalizaci√≥n': ['estructura', 'procesos', 'profesionalizar', 'orden', 'sistema'],\n  'Responsabilidad Social': ['impacto social', 'sostenible', '√©tica', 'medio ambiente'],\n  'Estrategia de Diversificaci√≥n': ['diversificaci√≥n', 'diversificar', 'nuevos productos', 'crecimiento', 'riesgo', 'expansi√≥n'],\n  'Estrategia de Producto': ['producto', 'prototipo', 'beta', 'desarrollo de producto'],\n  'Estrategias de Crecimiento': ['crecimiento', 'escalar', 'ampliar mercado'],\n  'Innovaci√≥n Estrat√©gica': ['innovaci√≥n', 'estrategia', 'disruptivo', 'transformaci√≥n'],\n};\n\n// === EVALUAR COINCIDENCIAS ===\nlet puntajes = {};\n\nfor (const categoria of categoriasInput) {\n  const keywords = mapaCategorias[categoria];\n  if (!keywords) continue;\n\n  let puntaje = 0;\n  for (const palabra of keywords) {\n    const re = new RegExp(`\\\\b${palabra}\\\\b`, 'gi');\n    puntaje += (texto.match(re) || []).length;\n  }\n\n  if (puntaje > 0) {\n    puntajes[categoria] = puntaje;\n  }\n}\n\n// === SELECCIONAR LA MEJOR CATEGOR√çA ===\nconst ordenadas = Object.entries(puntajes).sort((a, b) => b[1] - a[1]);\n\nif (ordenadas.length > 0) {\n  return [{ json: { categoriaSugerida: ordenadas[0][0], slug } }];\n} else {\n  return [{ json: { categoriaSugerida: 'Nueva categor√≠a (por definir)', slug } }];\n}"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[1120,432],"id":"972a4a12-0971-4f6a-99a0-bb7323edb76f","name":"definirCategoria"},{"parameters":{"method":"POST","url":"https://yessicamatus.com/wp-json/wp/v2/media","authentication":"predefinedCredentialType","nodeCredentialType":"wordpressApi","sendHeaders":true,"headerParameters":{"parameters":[{"name":"Content-Type","value":"image/png"},{"name":"Content-Disposition","value":"=attachment; filename=\"{{ $json['Nombre im√°gen'] }}\""}]},"sendBody":true,"contentType":"binaryData","inputDataFieldName":"img","options":{}},"type":"n8n-nodes-base.httpRequest","typeVersion":4.2,"position":[448,192],"id":"524abc8b-d1d7-4b50-8df4-a45faf0d7046","name":"HTTP Request2"},{"parameters":{"assignments":{"assignments":[{"id":"fe4f5ba7-f322-491d-a647-7ee21bfac103","name":"idImagen","value":"={{ $json.id }}","type":"number"}]},"options":{}},"type":"n8n-nodes-base.set","typeVersion":3.4,"position":[672,192],"id":"b468ec32-3a7d-4878-9c8f-65f8aceaf501","name":"Edit Fields"},{"parameters":{"method":"POST","url":"https://yessicamatus.com/wp-json/wp/v2/posts","authentication":"predefinedCredentialType","nodeCredentialType":"wordpressApi","sendBody":true,"bodyParameters":{"parameters":[{"name":"title","value":"={{ $json.Titulo }}"},{"name":"content","value":"={{ $json.contenido }}"},{"name":"excerpt","value":"={{ $json.Extracto }}"},{"name":"status","value":"future"},{"name":"categories","value":"={{ $json.idCategoria }}"},{"name":"date","value":"={{ $json.FechaPublicacionISO }}"},{"name":"featured_media","value":"={{ $json.idImagen }}"},{"name":"author","value":"2"},{"name":"slug","value":"={{ $json.slug }}"}]},"options":{}},"type":"n8n-nodes-base.httpRequest","typeVersion":4.2,"position":[2368,288],"id":"5b1c5f