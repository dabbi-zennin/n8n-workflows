Email = 'duplicado';\n    }\n\n    if (nextEmail) {\n      return {\n        json: {\n          ...item.json,\n          nextEmail,\n        }\n      };\n    }\n\n    return null;\n  })\n  .filter(item => item !== null);"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[-2448,752],"id":"c76949c3-ceed-4be3-aeec-2f4ff117b8d0","name":"Filtro","executeOnce":false},{"parameters":{"maxItems":"={{ $node[\"masterVar\"].json.envios }}"},"type":"n8n-nodes-base.limit","typeVersion":1,"position":[-2272,752],"id":"d3170be2-eee9-4020-803f-e28e00b50a20","name":"Limit"},{"parameters":{"assignments":{"assignments":[{"id":"59b03f17-220e-4474-8b91-db49f9b859d5","name":"Cliente","value":"Yessica Matus","type":"string"},{"id":"f769229f-796d-47bf-8fee-77b63f21d09f","name":"airtableBD","value":"appoIrpJlprLtsUFF","type":"string"},{"id":"06f2cbd4-0cbc-42cc-8da4-b6a12967fcf2","name":"Clientes_Variables","value":"tblzPRsNmPuFKgaqp","type":"string"},{"id":"337bf7e0-1109-40d2-85c1-91d4648539c4","name":"envios","value":1,"type":"number"}]},"options":{}},"type":"n8n-nodes-base.set","typeVersion":3.4,"position":[-3296,752],"id":"5c16ed09-3d7e-494b-a54a-024bb88330fc","name":"masterVar"},{"parameters":{"operation":"search","base":{"__rl":true,"value":"={{ $json.airtableBD }}","mode":"id"},"table":{"__rl":true,"value":"={{ $json.Clientes_Variables }}","mode":"id"},"filterByFormula":"={{ '{Cliente} = \"' + $json.Cliente + '\"' }}","options":{}},"type":"n8n-nodes-base.airtable","typeVersion":2.1,"position":[-3088,752],"id":"06c41e98-0b4f-46a3-9db2-aacb2e7de90f","name":"Cliente"},{"parameters":{"assignments":{"assignments":[{"id":"45406798-3793-4e2a-bf02-64749bb8d758","name":"Base","value":"={{ $json.Base }}","type":"string"},{"id":"30c47d20-a1ff-4006-8cf6-b8f241900fbd","name":"leadsReach","value":"={{ $json.leadsReach }}","type":"string"},{"id":"ef937799-b34d-4e63-b23f-c9f95e9e105a","name":"Cliente","value":"={{ $json.Cliente }}","type":"string"},{"id":"be818248-53a5-432e-b08b-537768e04501","