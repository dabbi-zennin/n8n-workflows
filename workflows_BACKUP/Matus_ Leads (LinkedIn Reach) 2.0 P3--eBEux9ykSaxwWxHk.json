dca-259a02fb0075","leftValue":"={{ $json.type }}","rightValue":"duplicate","operator":{"type":"string","operation":"equals","name":"filter.operator.equals"}}],"combinator":"and"}}]},"options":{}},"type":"n8n-nodes-base.switch","typeVersion":3.2,"position":[288,656],"id":"4d1bfad9-9f85-496a-9aec-0a409e1c9d78","name":"Switch1"},{"parameters":{"jsCode":"// 1) Normaliza entrada: por si viene como un array anidado\nlet inputItems = [];\nif (items.length === 1 && Array.isArray(items[0].json)) {\n  for (const row of items[0].json) {\n    inputItems.push({ json: row });\n  }\n} else {\n  inputItems = items;\n}\n\n// Usamos EMAIL como clave de deduplicación\nconst seen = new Set();\nconst uniqueItems = [];\nconst duplicateItems = [];\n\nfor (const item of inputItems) {\n  const row = item.json;\n  const email = (row.EMAIL || \"\").trim().toLowerCase();\n\n  if (email && seen.has(email)) {\n    // Duplicado: hay email y ya existe\n    duplicateItems.push({\n      json: {\n        ...row,\n        type: \"duplicate\"\n      }\n    });\n  } else {\n    // Único: no hay email o es la primera vez que se ve\n    if (email) {\n      seen.add(email);\n    }\n    uniqueItems.push({\n      json: {\n        ...row,\n        type: \"unique\"\n      }\n    });\n  }\n}\n\n// Ordena duplicados de mayor a menor row_number\nduplicateItems.sort((a, b) => b.json.row_number - a.json.row_number);\n\n// Combina únicos + duplicados ordenados\nconst outputItems = uniqueItems.concat(duplicateItems);\n\nreturn outputItems;\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[64,656],"id":"c606d1f7-0cf1-4810-8aa1-0e1df1b0f49a","name":"clasificarDuplicados"},{"parameters":{},"type":"n8n-nodes-base.noOp","typeVersion":1,"position":[512,576],"id":"fb071726-70e2-4262-a2f9-c8f40ba2a0c2","name":"No Operation, do nothing"},{"parameters":{},"type":"n8n-nodes-base.noOp","typeVersion":1,"position":[976,576],"id":"365d1734-04c4-4631-994b-b78b0610fcec","name":"No Operation, do nothing1"},{"parameters":{"r